{
  "name": "@peterek/helpers",
  "version": "1.0.2",
  "description": "Commonly used helper functions",
  "license": "MIT",
  "keywords": [
    "javascript",
    "typescript",
    "helpers"
  ],
  "author": {
    "name": "Jiri Peterek",
    "email": "jiri@peterek.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterekjs/helpers.git"
  },
  "bugs": {
    "url": "https://github.com/peterekjs/helpers/issues"
  },
  "homepage": "https://github.com/peterekjs/helpers#readme",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/helpers.cjs",
  "module": "./dist/helpers.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/helpers.cjs",
      "default": "./dist/helpers.js"
    },
    "./src/*": "./src/*"
  },
  "typesVersions": {
    ">=5": {
      "*": ["./dist/*.d.ts"]
    }
  },
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ts-essentials": "^9.4.1"
  },
  "peerDependencies": {
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.13",
    "@types/ramda": "^0.29.10",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2"
  }
}
