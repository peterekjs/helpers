{
  "name": "@peterek/helpers",
  "version": "1.0.0",
  "description": "Commonly used helper functions",
  "license": "MIT",
  "keywords": [
    "javascript",
    "typescript",
    "helpers"
  ],
  "author": {
    "name": "Jiri Peterek",
    "email": "jiri@peterek.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterekjs/helpers.git"
  },
  "bugs": {
    "url": "https://github.com/peterekjs/helpers/issues"
  },
  "homepage": "https://github.com/peterekjs/helpers#readme",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/helpers.cjs",
  "module": "./dist/helpers.js",
  "exports": {
    ".": {
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.cjs"
    },
    "./src/*": "./src/*"
  },
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@peterek/rollup-externals": "^1.0.0",
    "@types/node": "^18.15.11",
    "@types/ramda": "^0.28.8",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0"
  },
  "dependencies": {
    "ts-essentials": "^9.3.1"
  }
}
