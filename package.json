{
  "name": "@peterek/helpers",
  "version": "2.1.0",
  "description": "Commonly used helper functions",
  "license": "MIT",
  "keywords": [
    "javascript",
    "typescript",
    "helpers"
  ],
  "author": {
    "name": "Jiri Peterek",
    "email": "jiri@peterek.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterekjs/helpers.git"
  },
  "bugs": {
    "url": "https://github.com/peterekjs/helpers/issues"
  },
  "homepage": "https://github.com/peterekjs/helpers#readme",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/helpers.cjs",
  "module": "./dist/helpers.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/helpers.cjs",
      "default": "./dist/helpers.js"
    },
    "./src/*": "./src/*"
  },
  "typesVersions": {
    ">=5": {
      "index": [
        "./dist/index.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "coverage": "vitest run --coverage",
    "test:dev": "vitest",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "ramda": ">=0.29.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ramda": "^0.30.2",
    "@vitest/coverage-v8": "^2.1.8",
    "ramda": "^0.30.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
